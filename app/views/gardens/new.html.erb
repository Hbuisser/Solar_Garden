<div class="container">
  <%= simple_form_for @garden do |f| %>
	<%= f.input :title %>
	<%= f.input :description %>
	<%= f.input :address %>
	<%= f.input :price %>
	<%= f.input :square_meters %>
	<%= f.input :photo %>
	<%= f.input :photo_cache, as: :hidden %>

	<%= f.button :submit, "Add garden", class: "button" %>
  <% end %>
</div>
